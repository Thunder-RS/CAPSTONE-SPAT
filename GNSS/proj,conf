# Enable logging
CONFIG_LOG=y
CONFIG_LOG_DEFAULT_LEVEL=4

# Enable DK buttons and LEDs
CONFIG_DK_LIBRARY=y


# Enable LTE connection and networking
CONFIG_MODEM=y
CONFIG_MODEM_INFO=y
CONFIG_LTE_LINK_CONTROL=y
CONFIG_NETWORKING=y
CONFIG_NET_IPV4=y
CONFIG_NET_SOCKETS=y
CONFIG_NET_SOCKETS_OFFLOAD=y
CONFIG_NRF_MODEM_LIB=y
CONFIG_MODEM_INFO_ADD_NETWORK=y


# Enable GNSS (GPS)


# Enable nRF Cloud
CONFIG_NRF_CLOUD=y
CONFIG_NRF_CLOUD_MQTT=y
CONFIG_NRF_CLOUD_LOCATION=y



# Enable A-GPS


# Disable unnecessary features to reduce memory usage
CONFIG_MINIMAL_LIBC=y
CONFIG_NEWLIB_LIBC=n
